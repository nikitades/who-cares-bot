{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "longman/telegram-bot": "^0.72.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/uid": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Nikitades\\WhoCaresBot\\WebApi\\": "src/",
            "Longman\\TelegramBot\\Commands\\SystemCommands\\": "src/App/TelegramCommand/System/",
            "Longman\\TelegramBot\\Commands\\UserCommands\\": "src/App/TelegramCommand/User/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikitades\\WhoCaresBot\\WebApi\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fzaninotto/faker": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan": "^0.12.87",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-phpunit": "^0.12.19",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpstan/phpstan-symfony": "^0.12.33",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symplify/config-transformer": "^9.3",
        "thecodingmachine/phpstan-safe-rule": "^1.0"
    }
}
